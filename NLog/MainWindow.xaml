<Window x:Class="NLog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NLog"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button1" Content="Message 1" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="16" Width="100" Click="button1_Click"/>
        <Button x:Name="button2" Content="Message 2" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Width="100" FontSize="16" Click="button2_Click"/>

    </Grid>
</Window>
